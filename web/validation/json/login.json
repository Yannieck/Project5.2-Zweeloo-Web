{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://example.com/object1654162871.json",
  "title": "Login",
  "type": "object",
  "required": [
    "email",
    "password"
  ],
  "properties": {
    "email": {
      "$id": "#root/email",
      "title": "email",
      "type": "string",
      "format": "email",
      "minLength": 6,
      "maxLength": 127,
      "pattern": "^\\S+@\\S+\\.\\S+$",
      "examples": [
        "hello@nhlstenden.com"
      ]
    },
    "password": {
      "$id": "#root/password",
      "title": "password",
      "type": "string",
      "minLength": 8,
      "maxLength": 50,
      "default": "",
      "examples": [
        "test"
      ],
      "pattern": "^.*$"
    }
  }
}